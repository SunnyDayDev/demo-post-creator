<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FF4276C1</color>
    <color name="colorPrimary">#4276C1</color>
    <color name="colorPrimaryDark">#FF4276C1</color>
    <color name="divider">#1F000000</color>
    <color name="textDark">#2C2D2E</color>
    <color name="textDecorationSemiWhite">#40FFFFFF</color>
    <color name="textHintDark">#3D2C2D2E</color>
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
